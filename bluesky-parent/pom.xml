	<profiles>
		<profile>
			<id>localdev</id>
			<repositories>
				<repository>
					<id>bluesky-local-nexus</id>
					<url>http://192.168.99.100:8081/content/groups/public</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
					</snapshots>
				</repository>
			</repositories>
			<distributionManagement>
				<repository>
					<id>bluesky-releases</id>
					<url>http://192.168.99.100:8081/content/repositories/bluesky-releases</url>
				</repository>
				<snapshotRepository>
					<id>bluesky-snapshots</id>
